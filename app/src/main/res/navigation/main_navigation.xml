<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/instagram1">

    <activity
        android:id="@+id/mainActivity"
        app:defaultNavHost="true"
        android:name="com.example.instagramclone.MainActivity"
        app:navGraph="@navigation/main_navigation"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />

    <fragment
        android:id="@+id/instagram1"
        app:defaultNavHost="true"
        app:navGraph="@navigation/main_navigation"
        android:name="com.example.instagramclone.instagram1"
        android:label="fragment_instagram1"
        tools:layout="@layout/fragment_instagram1" />
    <fragment
        android:id="@+id/profile1"
        android:name="com.example.instagramclone.Profile1"
        android:label="fragment_profile1"
        tools:layout="@layout/fragment_profile1" />

</navigation>